{
  "mcpServers": {
    "supabase": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=REPLACE_WITH_YOUR_PROJECT_REF"
      ]
    },
    "memory": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "vercel": {
      "type": "http",
      "url": "https://mcp.vercel.com"
    },
    "railway": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@railway/mcp-server"
      ]
    },
    "cloudflare-docs": {
      "type": "http",
      "url": "https://docs.mcp.cloudflare.com/mcp"
    },
    "context7": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "magic": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@magicuidesign/mcp@latest"
      ]
    },
    "filesystem": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "REPLACE_WITH_HOME_DIR"
      ]
    },
    "gmail": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@gongrzhe/server-gmail-autoauth-mcp"
      ],
      "env": {}
    },
    "desktop-commander": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@wonderwhy-er/desktop-commander"
      ]
    },
    "github": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "REPLACE_WITH_YOUR_GITHUB_PAT"
      }
    },
    "b12": {
      "command": "node",
      "args": [
        "REPLACE_WITH_HOME_DIR/Projects/tools/website-generator-mcp-server/src/server.js"
      ]
    },
    "atum-audit": {
      "command": "cmd",
      "args": [
        "/c",
        "python",
        "C:\\Users\\arnau\\Desktop\\agent-owl\\atum_mcp_server.py"
      ],
      "env": {
        "ATUM_PROJECT_DIR": "C:\\Users\\arnau\\Desktop\\agent-owl"
      }
    },
    "webmcp": {
      "command": "node",
      "args": [
        "REPLACE_WITH_HOME_DIR/Projects/tools/webmcp-optimized/src/websocket-server.js",
        "--mcp"
      ]
    },
    "skillsync": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@stranzwersweb2/skillsync-mcp@latest"
      ]
    }
  }
}
